#!/bin/bash

. `dirname $0`/../etc/controller.bash.inc
autoload_functions "delitem_inarray"

echo '[testing] delitem_inarray()'
echo -n ' scenario 1 - '
target_string='quick brown fox jumps over lazy dog.'
dirty_string='the quick brown fox jumps over the lazy dog.'
[[ "$(delitem_inarray 'the' $dirty_string)" = "$target_string" ]] && echo "OK" || echo "fail"
